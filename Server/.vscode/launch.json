    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/main",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Cmake Debug",
            "type": "cppvsdbg",
            //"preLaunchTask": "build",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/main.exe",
            "args": ["-v", "-p 8000"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            //"miDebuggerPath": "C:\MinGW\bin\gdb.exe,
            //"preLaunchTask": "CMake"
        }

    ]
}